{
  "expo": {
    "name": "SwipeFund",
    "slug": "swipe-fund-daytrading",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/android-chrome-512x512.png",
    "splash": {
      "image": "./assets/android/drawable-hdpi/SwipeFundLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "plugins": ["@notifee/react-native"],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android/drawable-hdpi/SwipeFundLogo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.swipefund"
    },
    "web": {
      "favicon": "./assets/android-chrome-192x192.png"
    }
  }
}
